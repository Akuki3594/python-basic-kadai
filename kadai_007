
#リストの作成

weather_forecast = ["月曜日は晴れです", "火曜日は雨です", "水曜日は晴れです", "木曜日は晴れです", "金曜日は曇りです","土曜日は曇りのち雨です","日曜日は雷雨です"]

#リスト化できているかチェック
print(weather_forecast)

#リストの一部データを表示
print("リストから抽出　　　　：" + weather_forecast[2])

#ディクショナリの作成
weather = {"mon":"晴れ","tue":"雨","wed":"晴れ","thu":"晴れ","fri":"曇り","sat":"曇りのち雨","sun":"雷雨",}

#要素の情報を修正する
weather["wed"] = "水曜日は晴れです"

#ディクショナリの一部データを表示
print("ディクショナリから抽出：" + weather["wed"])
