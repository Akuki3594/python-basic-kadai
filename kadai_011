#forを用いた処理
l1 = [1,2]
l2 = ["水","金","地","火","木","土","天","海","冥"]

for i in l1:
    for j in l2:
        print(j)

#while
l2 = {1: "水", 2: "金", 3: "地", 4: "火", 5: "木", 6: "土", 7: "天", 8: "海", 9: "冥"}
num = 1

# ループ回数をカウントする変数を追加
loop_count = 0  

# ループ回数が2回未満の間
while loop_count < 2:  
    print(l2[num])
    num += 1

    if num > 9:
        num = 1
        loop_count += 1  # ループ回数をインクリメント
